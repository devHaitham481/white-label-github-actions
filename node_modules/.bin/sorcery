#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/exodus/Desktop/fricke/misc/whitelabel-hooks/node_modules/sorcery/bin/node_modules:/home/exodus/Desktop/fricke/misc/whitelabel-hooks/node_modules/sorcery/node_modules:/home/exodus/Desktop/fricke/misc/whitelabel-hooks/node_modules:/home/exodus/Desktop/fricke/misc/node_modules:/home/exodus/Desktop/fricke/node_modules:/home/exodus/Desktop/node_modules:/home/exodus/node_modules:/home/node_modules:/node_modules:/home/exodus/Desktop/fricke/misc/whitelabel-hooks/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/exodus/Desktop/fricke/misc/whitelabel-hooks/node_modules/sorcery/bin/node_modules:/home/exodus/Desktop/fricke/misc/whitelabel-hooks/node_modules/sorcery/node_modules:/home/exodus/Desktop/fricke/misc/whitelabel-hooks/node_modules:/home/exodus/Desktop/fricke/misc/node_modules:/home/exodus/Desktop/fricke/node_modules:/home/exodus/Desktop/node_modules:/home/exodus/node_modules:/home/node_modules:/node_modules:/home/exodus/Desktop/fricke/misc/whitelabel-hooks/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../sorcery/bin/sorcery" "$@"
else
  exec node  "$basedir/../sorcery/bin/sorcery" "$@"
fi
